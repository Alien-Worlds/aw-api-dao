{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "profile.response.schema.json",
    "title": "/:dacId/profile?account={walletId} response schema",
    "type": "object",
    "properties": {
        "results": {
            "type": "array",
            "minItems": 1,
            "items": {
                "type": "object",
                "properties": {
                    "block_num": {
                        "type": "string"
                    },
                    "account": {
                        "type": "string"
                    },
                    "profile": {
                        "type": "object",
                        "properties": {
                            "description": {
                                "type": "string"
                            },
                            "familyName": {
                                "type": "string"
                            },
                            "givenName": {
                                "type": "string"
                            },
                            "image": {
                                "type": "string"
                            }
                        },
                        "required": [
                            "description",
                            "givenName",
                            "image"
                        ]
                    }
                },
                "required": [
                    "block_num",
                    "account",
                    "profile"
                ]
            }
        },
        "count": {
            "type": "integer"
        }
    },
    "required": [
        "results",
        "count"
    ]
}
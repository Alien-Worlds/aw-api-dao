{
  "name": "eosdac-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "api": "node -r ts-node/register -r tsconfig-paths/register src/index.ts",
    "api:dev": "tsc-watch --onSuccess \"node -r ts-node/register -r tsconfig-paths/register src/index.ts\"",
    "test:unit": "jest --config=jest.config.unit.ts",
    "test:api": "jest --config=jest.config.api.ts --forceExit --runInBand",
    "clean": "rm -rf ./dist",
    "build": "yarn clean && tsc",
    "lint": "eslint \"src/**/*\" \"tests/**/*\"",
    "lint:fix": "eslint --fix \"src/**/*\" \"tests/**/*\"",
    "prettier:check": "yarn prettier --check \"src/**/*\" \"tests/**/*\"",
    "prettier:fix": "yarn prettier --write \"src/**/*\" \"tests/**/*\"",
    "format": "yarn prettier:fix && yarn lint:fix"
  },
  "dependencies": {
    "@alien-worlds/api-core": "0.0.69",
    "@alien-worlds/eosdac-api-common": "0.0.77",
    "@jafri/eosjs2": "^20.0.1-e34c16e.170",
    "@shelf/winston-datadog-logs-transport": "^1.0.7",
    "dotenv": "^16.0.2",
    "fastify": "^3.25.0",
    "fastify-cors": "^6.0.2",
    "fastify-oas": "^3.0.8",
    "reflect-metadata": "^0.1.13",
    "ts-mockito": "^2.6.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/jest": "^27.4.5",
    "@types/node": "^18.7.15",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "jest": "27.4.5",
    "prettier": "^2.7.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.3"
  }
}

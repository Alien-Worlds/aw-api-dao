{
  "name": "eosdac-api",
  "version": "1.0.0",
  "main": "index.js",
  "author": "<michael@eosdac.io>",
  "license": "MIT",
  "scripts": {
    "api": "node -r ts-node/register -r tsconfig-paths/register src/index.ts",
    "api:dev": "tsc-watch --onSuccess \"node -r ts-node/register -r tsconfig-paths/register src/eosdac-api.ts\"",
    "test:api": "jest --config=jest.config.api.ts --forceExit",
    "clean": "rm -rf ./dist",
    "build": "yarn clean && tsc",
    "lint": "eslint \"src/**/*\" \"tests/**/*\"",
    "lint:fix": "eslint --fix \"src/**/*\" \"tests/**/*\"",
    "prettier:check": "yarn prettier --check \"src/**/*\" \"tests/**/*\"",
    "prettier:fix": "yarn prettier --write \"src/**/*\" \"tests/**/*\"",
    "format": "yarn prettier:fix && yarn lint:fix"
  },
  "dependencies": {
    "@eosdacio/eosio-statereceiver": "^1.0.4",
    "@jafri/eosjs2": "^20.0.1-e34c16e.170",
    "@shelf/winston-datadog-logs-transport": "^1.0.7",
    "amqplib": "^0.5.3",
    "array-xor": "^0.1.1",
    "commander": "^9.4.0",
    "dirty-json": "^0.7.0",
    "dotenv": "^16.0.2",
    "elasticsearch": "^15.3.0",
    "fastify": "^3.25.0",
    "fastify-autoload": "^0.6.0",
    "fastify-cors": "^2.1.3",
    "fastify-mongodb": "^1.0.1",
    "fastify-oas": "^3.0.8",
    "int64-buffer": "^0.99.1007",
    "mongodb": "^3.1",
    "nan": "^2.4.0",
    "node-fetch": "^2.2.0",
    "node-ipc": "^9.1.1",
    "path": "^0.12.7",
    "text-encoding": "^0.6.4",
    "websocket": "^1.0.30",
    "winston": "^3.2.1",
    "ws": "^6.0.0",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@types/amqplib": "^0.8.2",
    "@types/jest": "^27.4.5",
    "@types/node": "^18.7.15",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "jest": "27.4.5",
    "prettier": "^2.7.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.3"
  }
}